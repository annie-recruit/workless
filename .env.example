# Sentry (로컬 확인용: DSN만 있으면 동작)
# - 브라우저(클라이언트) 전송용
NEXT_PUBLIC_SENTRY_DSN=
# - 서버/엣지/Route Handler 전송용
SENTRY_DSN=

# (선택) 성능 트레이싱 샘플링 (0~1)
NEXT_PUBLIC_SENTRY_TRACES_SAMPLE_RATE=0.1
SENTRY_TRACES_SAMPLE_RATE=0.1

# (선택) 환경/릴리즈
NEXT_PUBLIC_SENTRY_ENVIRONMENT=development
SENTRY_ENVIRONMENT=development
NEXT_PUBLIC_SENTRY_RELEASE=
SENTRY_RELEASE=

# (선택) 배포에서 소스맵 업로드용
SENTRY_AUTH_TOKEN=
SENTRY_ORG=
SENTRY_PROJECT=

# (권장) Sentry Webhook 서명 검증용 시크릿 (Sentry Webhooks 설정에 동일하게 입력)
SENTRY_WEBHOOK_SECRET=
